{
  "name": "airdrop-finder",
  "version": "1.0.0",
  "private": true,
  "description": "Airdrop Finder - Check wallet eligibility for airdrops",
  "scripts": {
    "dev": "pnpm --filter @airdrop-finder/web dev",
    "build": "pnpm --filter @airdrop-finder/web build",
    "start": "pnpm --filter @airdrop-finder/web start",
    "lint": "pnpm --filter @airdrop-finder/web lint",
    "clean": "pnpm -r exec rm -rf node_modules .next dist"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}

